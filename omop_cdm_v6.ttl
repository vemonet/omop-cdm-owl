            owl:allValuesFrom omop:Concept ;
            owl:onProperty omop:has_concept ],
        [ a owl:Restriction ;
            owl:onProperty omop:has_concept ;
            owl:someValuesFrom omop:Concept ],
        [ a owl:Restriction ;
            owl:allValuesFrom omop:Concept ;
            owl:onProperty omop:has_language ],
        [ a owl:Restriction ;
            owl:onProperty omop:has_language ;
            owl:someValuesFrom omop:Concept ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty omop:name ],
        [ a owl:Restriction ;
            owl:onProperty omop:name ;
            owl:someValuesFrom xsd:string ],
        omop:OmopCDMThing ;
    omop:omop_cdm_name "concept_synonym"^^xsd:string .

omop:ConditionEra a owl:Class ;
    rdfs:label "Condition era"^^xsd:string ;
    rdfs:comment """A Condition Era is defined as a span of time when the Person is assumed to have a given condition. Similar to Drug Eras, Condition Eras are chronological periods of Condition Occurrence. Combining individual Condition Occurrences into a single Condition Era serves two purposes:

- It allows aggregation of chronic conditions that require frequent ongoing care, instead of treating each Condition Occurrence as an independent event.
- It allows aggregation of multiple, closely timed doctor visits for the same Condition to avoid double-counting the Condition Occurrences.
For example, consider a Person who visits her Primary Care Physician (PCP) and who is referred to a specialist. At a later time, the Person visits the specialist, who confirms the PCP's original diagnosis and provides the appropriate treatment to resolve the condition. These two independent doctor visits should be aggregated into one Condition Era."""@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf omop:has_condition_era ] ;
            owl:someValuesFrom omop:Person ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty omop:occurrence_count ],
        omop:Era ;
    omop:omop_cdm_name "condition_era"^^xsd:string .

omop:ObservationPeriod a owl:Class ;
    rdfs:label "Observation period"^^xsd:string ;
    rdfs:comment "This table contains records which define spans of time during which two conditions are expected to hold: (i) Clinical Events that happened to the Person are recorded in the Event tables, and (ii) absense of records indicate such Events did not occur during this span of time."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty omop:has_period_type ;
            owl:someValuesFrom omop:Concept ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf omop:has_observation_period ] ;
            owl:someValuesFrom omop:Person ],
        [ a owl:Restriction ;
            owl:allValuesFrom omop:Concept ;
            owl:onProperty omop:has_period_type ],
        omop:ClinicalElement,
        omop:DateDuration ;
    omop:omop_cdm_name "observation_period"^^xsd:string .

omop:admitted_from_source_value a owl:DatatypeProperty,
