omop:VisitDetail a owl:Class ;
    rdfs:label "Visit detail"^^xsd:string ;
    rdfs:comment "The VISIT_DETAIL table is an optional table used to represents details of each record in the parent VISIT_OCCURRENCE table. A good example of this would be the movement between units in a hospital during an inpatient stay or claim lines associated with a one insurance claim. For every record in the VISIT_OCCURRENCE table there may be 0 or more records in the VISIT_DETAIL table with a 1:n relationship where n may be 0. The VISIT_DETAIL table is structurally very similar to VISIT_OCCURRENCE table and belongs to the visit domain."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom omop:Concept ;
            owl:onProperty omop:has_admitted_from ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty omop:admitted_from_source_value ],
        [ a owl:Restriction ;
            owl:onProperty omop:admitted_from_source_value ;
            owl:someValuesFrom xsd:integer ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf omop:has_visit_detail ] ;
            owl:someValuesFrom omop:Person ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf omop:has_visit_detail ] ;
            owl:someValuesFrom omop:VisitOccurrence ],
        [ a owl:Restriction ;
            owl:allValuesFrom omop:VisitDetail ;
            owl:onProperty omop:has_preceding_visit_detail ],
        [ a owl:Restriction ;
            owl:allValuesFrom omop:VisitDetail ;
            owl:onProperty omop:has_parent ],
        omop:BaseVisit ;
    omop:omop_cdm_name "visit_detail"^^xsd:string .

