        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty omop:source_documentation_reference ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty omop:cdm_etl_reference ],
        [ a owl:Restriction ;
